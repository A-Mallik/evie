{  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "version": "1.0.0",
  "name": "EVI",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.js",
        "content.js",
        "sentiment.js"
      ],
      "run_at": "document_end"
    }
  ]
}
